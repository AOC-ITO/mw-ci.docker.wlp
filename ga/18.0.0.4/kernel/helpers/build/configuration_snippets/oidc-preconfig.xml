<?xml version="1.0" encoding="UTF-8"?>
<server>
    <openidConnectClient id="rp"
         clientId="${env.OIDC_CLIENT_ID}"
         clientSecret="${env.OIDC_CLIENT_SECRET}"
         issuerIdentifier="https://${env.OIDC_INGRESS_HOST}:${env.OIDC_INGRESS_PORT}/oidc/endpoint/OIDC"         
         scope="openid profile email photo"
         signatureAlgorithm="RS256" 
         discoveryEndpointUrl="https://${env.OIDC_INGRESS_HOST}:${env.OIDC_INGRESS_PORT}/oidc/.well-known/openid-configuration">
      </openidConnectClient>
</server>
